colvarsTrajFrequency      5000             
colvarsRestartFrequency   5000            
indexFile                 ../complex.ndx      
colvar {                                    
    name RMSD                                
    width 0.05                               
    lowerboundary 0.00            
    upperboundary 10.00            
    rmsd {                                  
        atoms {                             
            indexGroup  ligand               
        }                                   
        refpositionsfile  ../complex.xyz          
    }                                       
}                                           
colvar {                              
    name eulerTheta                   
    customFunction asin(2 * (q1*q3-q4*q2)) * 180 / 3.1415926
    width 1                            
    lowerboundary -90.0      
    upperboundary 90.0      
    Orientation {                             
        name  q                                
        atoms {                               
            indexGroup  ligand                 
            centerReference    on              
            rotateReference    on              
	        enableFitGradients no              
            fittingGroup {                    
                indexGroup  protein            
            }                                 
            refpositionsfile  ../complex.xyz        
         }                                    
         refpositionsfile  ../complex.xyz           
    }                                         
}                                             
colvar {                              
    name eulerPhi                   
    customFunction atan2(2*(q1*q2+q3*q4), 1-2*(q2*q2+q3*q3)) * 180 / 3.1415926
    width 1                            
    lowerboundary -180.0      
    upperboundary 180.0      
    Orientation {                             
        name  q                                
        atoms {                               
            indexGroup  ligand                 
            centerReference    on              
            rotateReference    on              
	        enableFitGradients no              
            fittingGroup {                    
                indexGroup  protein            
            }                                 
            refpositionsfile  ../complex.xyz        
         }                                    
         refpositionsfile  ../complex.xyz           
    }                                         
}                                             
colvar {                              
    name eulerPsi                   
    customFunction atan2(2*(q1*q4+q2*q3), 1-2*(q3*q3+q4*q4)) * 180 / 3.1415926
    width 1                            
    lowerboundary -180.0      
    upperboundary 180.0      
    Orientation {                             
        name  q                                
        atoms {                               
            indexGroup  ligand                 
            centerReference    on              
            rotateReference    on              
	        enableFitGradients no              
            fittingGroup {                    
                indexGroup  protein            
            }                                 
            refpositionsfile  ../complex.xyz        
         }                                    
         refpositionsfile  ../complex.xyz           
    }                                         
}                                             
colvar {                                   
    name polarTheta                            
    customFunction acos(-i2) * 180 / 3.1415926
    width 1                                 
    lowerboundary 0.0           
    upperboundary 180.0           
    distanceDir {                          
        name  i                             
        group1 {                           
            indexGroup  reference           
            centerReference    on           
            rotateReference    on           
            enableFitGradients no           
            fittingGroup {                 
                indexGroup  protein         
            }                              
            refpositionsfile  ../complex.xyz     
        }                                  
        group2 {                           
            indexGroup  ligand              
            centerReference    on           
            rotateReference    on           
            enableFitGradients no           
            fittingGroup {                 
                indexGroup  protein         
            }                              
            refpositionsfile  ../complex.xyz     
        }                                  
    }                                      
}                                          
colvar {                                   
    name polarPhi                            
    customFunction atan2(i3, i1) * 180 / 3.1415926
    period  360                             
    wrapAround 0.0                          
    width 1                                 
    lowerboundary -180.0           
    upperboundary 180.0           
    distanceDir {                          
        name  i                             
        group1 {                           
            indexGroup  reference           
            centerReference    on           
            rotateReference    on           
            enableFitGradients no           
            fittingGroup {                 
                indexGroup  protein         
            }                              
            refpositionsfile  ../complex.xyz     
        }                                  
        group2 {                           
            indexGroup  ligand              
            centerReference    on           
            rotateReference    on           
            enableFitGradients no           
            fittingGroup {                 
                indexGroup  protein         
            }                              
            refpositionsfile  ../complex.xyz     
        }                                  
    }                                      
}                                          
colvar {                            
    name    r                        
    width 0.10                        
    lowerboundary 14.00    
    upperboundary 41.00    
    distance {                            
        forceNoPBC       yes               
        group1 {                          
            indexGroup  reference          
	    }                                 
        group2 {                          
            indexGroup  ligand             
        }                                 
    }                                     
}                                         
histogram {                     
  colvars    RMSD                
  outputFileDX  none             
  outputFreq 10000               
}                               
histogram {                     
  colvars    eulerTheta                
  outputFileDX  none             
  outputFreq 10000               
}                               
histogram {                     
  colvars    eulerPhi                
  outputFileDX  none             
  outputFreq 10000               
}                               
histogram {                     
  colvars    eulerPsi                
  outputFileDX  none             
  outputFreq 10000               
}                               
histogram {                     
  colvars    polarTheta                
  outputFileDX  none             
  outputFreq 10000               
}                               
histogram {                     
  colvars    polarPhi                
  outputFileDX  none             
  outputFreq 10000               
}                               
histogram {                     
  colvars    r                
  outputFileDX  none             
  outputFreq 10000               
}                               
colvar {                         
  name translation                
  distance {                     
    group1 {                     
      indexGroup  protein         
    }                            
    group2 {                     
      dummyAtom (-1.3068323135375977, -0.030426498502492905, 0.4462643563747406)    
    }                            
  }                              
}                                
harmonic {                       
  colvars       translation       
  centers       0.0               
  forceConstant 100.0             
}                                
                                  
colvar {                         
  name orientation                
  orientation {                  
    atoms {                      
      indexGroup  protein         
    }                            
    refPositionsFile   ../complex.xyz  
  }                              
}                                
harmonic {                       
  colvars       orientation       
  centers       (1.0, 0.0, 0.0, 0.0)    
  forceConstant 2000.0            
}                                
